<%- include('./partials/header.ejs') %>
<%- include('./partials/navbar.ejs') %>
<style>
  body{
    background: rgba(2, 0, 36, 0.568);
  background: linear-gradient(90deg, rgba(91, 88, 139, 0.479) 0%, rgba(9, 9, 121, 0.445) 35%, rgba(0, 213, 255, 0.438) 100%);}
  .tab{
    overflow-x: scroll;
  }
</style>

    <table class="table">
        <thead class="table-success">
         <tr class="text-center">
           <td>Phone </td>
             <td>Name </td>
         </tr>
        </thead>
        <tbody>
            <% for(var i=0; i < agents.length; i++) { %>
                <tr class="text-center">
                  <td><%= agents[i].phone %></td>
                  <td><%= agents[i].first_name %></td>
                </tr>
             <% } %>
        </tbody>
      </table>
<hr>

<div class="tab">
  

    <table class="table mt-4">
        <thead class="table-success">
         <tr class="text-center">
           <td>Phone </td>
           <td>Name </td>
           <td>Account No </td>
           <td>Status </td>
           <td>Actions </td>
         </tr>
        </thead>
        <tbody>
            <% for(var i=0; i < payouts.length; i++) { %>
                <tr class="text-center">
                  <td><%= payouts[i].phone %></td>
                  <td><%= payouts[i].name %></td>
                  <td><%= payouts[i].account_number %></td>
                  <td><%= payouts[i].payout_status %></td>
                  <td class="d-flex"><a href="/user/agent/approve_payout/<%= payouts[i].id %>" class="btn btn-sm btn-success px-2">Approve</a>
                  <a href="/user/agent/reject_payout/<%= payouts[i].id %>" class="btn btn-sm ms-2 btn-danger px-2">Reject</a>
                  </td>
                </tr>
             <% } %>
        </tbody>
      </table>
</div>






<%- include('./partials/footer.ejs') %>